{
  "name": "tabstart",
  "description": "A new tab page replacement",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "browser_action": {
    "default_title": "tabstart",
    "default_popup": "index.html"
  },
   "chrome_url_overrides": {
      "newtab": "index.html"
    },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
