
{
  "manifest_version": 2,
  "name": "Tabstart",
  "version": "1.0",
  "description": "New tab page change, by Frostq",

   "background": {
    "scripts": [
      "js/jquery.min.js"
  ],
  
  "content_scripts": [
    {
        "matches": ["http://*/*"],
        "js": ["jquery.min.js"]
    }
  ],
  
  "browser_action": {
    "default_icon": "logo.png"
  },

    "chrome_url_overrides": {
      "newtab": "index.html"
    },


  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}
