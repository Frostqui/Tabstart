{
  "manifest_version": 2,
  "name": "Tabstart",
  "version": "1.0",
  "description": "New tab page change, by Frostq",

   "background": {
    "scripts": [
      "js/jquery.min.js"
    ]
    },
  

  


    "chrome_url_overrides": {
      "newtab": "index.html"
    },


  "permissions": [
    "activeTab",
    "topSites",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}
